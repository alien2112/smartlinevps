                            "raw": "{\n  \"phone\": \"01099999999\",\n  \"vehicle_category_id\": \"category_id_here\",\n  \"travel_enabled\": false\n}"
                        }
                    }
                },
                {
                    "name": "Step 6: Upload Document",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/driver/auth/upload/{type}",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "multipart/form-data"
                            }
                        ],
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "phone",
                                    "value": "01099999999",
                                    "type": "text"
                                },
                                {
                                    "key": "file",
                                    "type": "file"
                                }
                            ]
                        },
                        "description": "Types: id_front, id_back, license_front, license_back, car_front, car_back, selfie"
                    }
                },
                {
                    "name": "Get Status (Resume)",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/driver/auth/status?phone=01099999999",
                        "description": "Check onboarding status - Flutter calls this on app open"
                    }
                },
