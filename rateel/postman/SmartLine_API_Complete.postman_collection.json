                            "raw": "{\n  \"phone\": \"01099999999\",\n  \"first_name_ar\": \"أحمد\",\n  \"last_name_ar\": \"محمد\",\n  \"national_id\": \"12345678901234\",\n  \"city_id\": \"1\"\n}"
                        }
                    }
                },
                {
                    "name": "Step 5: Vehicle Type",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/driver/auth/vehicle-type",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
